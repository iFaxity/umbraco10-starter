@inherits AdvaniaViewPage
@{
    Layout = "~/Views/Layouts/_Root.cshtml";
}

@section BeforeBody {
    @RenderSection("BeforeBody", false)
}

@await Html.PartialAsync("_Header")
@await Html.PartialAsync("_Breadcrumbs")

<div class="flex-1 mb-32">
    @RenderBody()
</div>

@await Html.PartialAsync("_Footer")

@section AfterBody {
    @RenderSection("AfterBody", false)
}
