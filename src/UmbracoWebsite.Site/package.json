{
  "name": "site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:css": "tailwindcss --postcss -i ./Assets/Styles/style.css -o ./wwwroot/dist/style.css",
    "watch": "vite build --watch",
    "watch:css": "tailwindcss --watch --postcss -i ./Assets/Styles/style.css -o ./wwwroot/dist/style.css",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^17.0.19",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-pxtorem": "^6.0.0",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.5",
    "vite": "^2.7.13"
  },
  "dependencies": {
    "plyr": "^3.7.2"
  }
}
